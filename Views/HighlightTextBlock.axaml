<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="CheckHash.Views.Controls.HighlightTextBlock"
             x:Name="Root">
    <TextBlock Name="PART_TextBlock"
               TextTrimming="{Binding #Root.TextTrimming}"
               FontWeight="{Binding #Root.FontWeight}"
               FontSize="{Binding #Root.FontSize}"
               Foreground="{Binding #Root.Foreground}"
               HorizontalAlignment="{Binding #Root.HorizontalAlignment}"
               VerticalAlignment="{Binding #Root.VerticalAlignment}" />
</UserControl>